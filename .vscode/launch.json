{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Deno",
      "type": "node",
      "request": "launch",
      "program": "${file}",
      "cwd": "${fileDirname}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["run", "--inspect-brk", "--allow-net", "--allow-read"],
      "attachSimplePort": 9229
    },
    {
      "name": "Attach to Deno",
      "type": "node",
      "request": "attach",
      "port": 9229
    }
  ]
}
